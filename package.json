{
	"name": "demo-express-api",
	"version": "1.0.0",
	"description": "A demo express api written in node and typescript",
	"main": "index.ts",
	"scripts": {
		"build-ts": "tsc",
		"build": " npm run lint && npm run build-ts",
		"serve": "node dist/index.js",
		"start": "npm run serve",
		"dev": "cross-env NODE_ENV=development nodemon src/index.ts",
		"test": "jest",
		"test:watch": "jest -i --watchAll",
		"coverage": "jest -i --coverage",
		"lint": "eslint \"**/*.{js,ts}\"",
		"lint:fix": "eslint \"**/*.{js,ts}\" --quiet --fix",
		"prettier": "prettier --check **/*.ts",
		"prettier:fix": "prettier --write **/*.ts",
		"prepare": "husky install",
		"docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
		"docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/base-64": "^1.0.0",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.13",
		"@types/http-errors": "^1.8.1",
		"@types/jest": "^27.0.2",
		"@types/joi": "^17.2.3",
		"@types/morgan": "^1.9.3",
		"@types/morgan-json": "^1.1.0",
		"@types/node": "^16.10.6",
		"@types/shelljs": "^0.8.9",
		"@types/supertest": "^2.0.11",
		"@types/uuid": "^8.3.1",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"eslint": "8.0.0",
		"eslint-config-prettier": "^8.3.0",
		"husky": "^7.0.2",
		"jest": "^27.3.1",
		"nock": "^13.2.4",
		"node-mocks-http": "^1.11.0",
		"nodemon": "^2.0.13",
		"prettier": "^2.4.1",
		"shelljs": "^0.8.4",
		"supertest": "^6.1.6",
		"ts-jest": "^27.0.7",
		"ts-node": "^10.2.1",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.5.5"
	},
	"dependencies": {
		"base-64": "^1.0.0",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"eslint-plugin-prettier": "^4.0.0",
		"express": "^4.17.2",
		"express-prom-bundle": "^6.4.1",
		"express-validator": "^6.14.0",
		"got": "^11.8.2",
		"helmet": "^4.6.0",
		"http-errors": "^1.8.1",
		"http-status": "^1.5.0",
		"joi": "^17.6.0",
		"morgan": "^1.10.0",
		"morgan-json": "^1.1.0",
		"prom-client": "^14.0.1",
		"uuid": "^8.3.2",
		"winston": "^3.3.3"
	}
}
